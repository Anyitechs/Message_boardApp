<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <% messages.forEach(message => { %>
      <div class="messages">
        <img src="/public/images/avatar.png" alt="Avatar" >
        <div class="container">
          <h4> <b> <%= message.text %> </b> </h4>
          <p> From: <%= message.user %> </p>
          <p> at:  <%= message.added %> </p>
        </div>
      </div>
    <% }); %>
    <div>
    </div>
    <a href="/new">Create a new message</a>
  </body>
</html>
